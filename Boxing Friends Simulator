local Window = OrionLib:MakeWindow({IntroText = "ðŸ‘‘Suriaj HubðŸ‘‘", Name = "ðŸ¥ŠBoxing Friend Simulator", HidePremium = false, SaveConfig = true, ConfigFolder = "ðŸ‘‘SuriajðŸ‘‘"})

-- Value
_G.autoBiceps = false
_G.autoKnuckles = false
_G.autoWins = false
