local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({IntroText = "üëëSuriaj Hubüëë", Name = "ü•äBoxing Friend Simulator", HidePremium = false, SaveConfig = true, ConfigFolder = "üëëSuriajüëë"})

-- Value
_G.autoBiceps = false
_G.autoKnuckles = false
_G.autoWins = false
_G.autoRebirth = false
_G.autoTripleHatch = false
_G.autoHatch = false
_G.fastAutoHatch = false
_G.autoCraft = false
_G.autoChest = false
_G.antiAfk = false
_G.autoChest = false
_G.autoSpin = false
_G.autoCrasher = false

-- Functions
function autoBiceps()
    while _G.autoBiceps == true do
        game:GetService("ReplicatedStorage").Events.Weight:FireServer("WeightTool")
        wait()
    end
end

function autoKnuckles()
    while _G.autoKnuckles == true do
        game:GetService("ReplicatedStorage").Events.Knuckle:FireServer("27")
        wait()
    end
end

function autoWins()
    while _G.autoWins == true do
        game:GetService("ReplicatedStorage").Events.NPC:FireServer("Kill", "Level 20")
        wait(1)
    end
end

function autoRebirth()
    while _G.autoRebirth == true do
        game:GetService("ReplicatedStorage").Events.Rebirth:FireServer()
        wait(.1)
    end
end

function tripleAutoHatch()
    while _G.tripleAutoHatch == true do
local ohString1 = _G.selectEgg
local ohNumber2 = 3
game:GetService("ReplicatedStorage").Events.HatchEgg:InvokeServer(ohString1, ohNumber2)
        wait()
    end
end

function autoCraft()
    while _G.autoCraft == true do

game:GetService("ReplicatedStorage").Events.PetAction:InvokeServer("Craft All")
        wait()
    end
end

function autoHatch()
    while _G.autoHatch == true do
local ohString1 = _G.selectEgg
local ohNumber2 = 1
game:GetService("ReplicatedStorage").Events.HatchEgg:InvokeServer(ohString1, ohNumber2)
        wait()
    end
end

