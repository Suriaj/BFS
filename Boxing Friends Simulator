	while _G.autoCrasher == true do
		local ohBoolean1 = true
		game:GetService("ReplicatedStorage").Combat.Events["Burning Punch"]:FireServer(ohBoolean1)
		game:GetService("ReplicatedStorage").Combat.Events["Burning Punch"]:FireServer(ohBoolean1)
		game:GetService("ReplicatedStorage").Combat.Events["Burning Punch"]:FireServer(ohBoolean1)
